{
  "name": "tool-box",
  "version": "1.0.0",
  "description": "脚手架",
  "main": "src/main.js",
  "scripts": {
    "debug": "cd client && cross-env NODE_ENV=debug webpack-dev-server --config build/webpack.config.dev.js",
    "dev": "cd client && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.dev.js",
    "build": "npm run clean && cd client && cross-env NODE_ENV=production webpack --config build/webpack.config.prod.js",
    "clean": "rimraf dist",
    "lint": "cd client && eslint --ext .js --ext .vue src/",
    "lint-fix": "cd client && eslint --fix --ext .js --ext .vue src/ debug/",
    "precommit": "npm run lint-fix",
    "server-dev": "cd server && nodemon server.js",
    "server-test": "npm run build && cd server && node server.js test",
    "server-release": "npm run build && cd server && node server.js release",
    "deploy-test": "pm2 start pm2-test.json",
    "deploy-release": "pm2 start pm2-release.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hellobeifeng/tool-box.git"
  },
  "author": "feng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hellobeifeng/tool-box/issues"
  },
  "homepage": "https://github.com/hellobeifeng/tool-box#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "compression": "^1.7.1",
    "ejs": "^2.5.7",
    "element-ui": "^2.0.11",
    "express": "^4.16.3",
    "jquery": "^3.1.0",
    "js-cookie": "^2.1.4",
    "pm2": "^2.9.3",
    "vue": "2.5.13",
    "vue-codemirror": "^4.0.5",
    "vue-cookie": "^1.1.3",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "7.2.3",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-vue-jsx": "3.5.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.7",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "husky": "^0.14.3",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "postcss-loader": "2.0.9",
    "rimraf": "^2.6.2",
    "style-loader": "0.19.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "url-loader": "0.6.2",
    "vue-loader": "13.6.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "2.5.13",
    "vuedraggable": "^2.16.0",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
